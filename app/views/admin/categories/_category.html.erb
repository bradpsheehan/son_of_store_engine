<thead>
  <tr>
    <th><%= category.title %> <%= link_to t('crud.edit'), edit_store_admin_category_path(current_store, category) %></th>
  </tr>
</thead>
<tbody>
  <% category.products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
    </tr>
  <% end %>
  <% if category.products.count == 0 %>
    <tr>
      <td><%= t('categories.noproducts') %></td>
    </tr>
  <% end %>
</tbody>
